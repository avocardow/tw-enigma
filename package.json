{
  "name": "tw-enigma",
  "version": "0.1.0",
  "description": "A post-build optimizer for Tailwind CSS that bundles utility classes into single, obfuscated identifiers.",
  "author": "Rowan Cardow",
  "license": "MIT",
  "homepage": "https://github.com/avocardow/tw-enigma",
  "bugs": {
    "url": "https://github.com/avocardow/tw-enigma/issues"
  },
  "keywords": [
    "tailwind",
    "tailwindcss",
    "css",
    "optimizer",
    "bundle",
    "obfuscate",
    "postcss"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avocardow/tw-enigma.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "enigma": "dist/bin/enigma.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm test && pnpm build",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build && changeset publish",
    "pack:validate": "npm pack --dry-run",
    "list": "node scripts/dev.js list",
    "generate": "node scripts/dev.js generate",
    "parse-prd": "node scripts/dev.js parse-prd",
    "license:add": "node scripts/add-license-headers.js",
    "license:check": "node scripts/add-license-headers.js --dry-run --verbose"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.54.0",
    "@changesets/cli": "^2.29.4",
    "@types/chokidar": "^2.1.7",
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "cheerio": "^1.0.0-rc.12",
    "chokidar": "^4.0.3",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.0",
    "cors": "^2.8.5",
    "cosmiconfig": "8.1.3",
    "critical": "^7.2.1",
    "cssnano": "^7.0.7",
    "domhandler": "^5.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "fastmcp": "^1.20.5",
    "figlet": "^1.8.0",
    "fuse.js": "^7.0.0",
    "glob": "^11.0.2",
    "gradient-string": "^3.0.0",
    "helmet": "^8.1.0",
    "inquirer": "^12.5.0",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^10.2.0",
    "openai": "^4.89.0",
    "ora": "^8.2.0",
    "penthouse": "^2.3.3",
    "puppeteer": "^24.10.0",
    "source-map": "^0.7.4",
    "uuid": "^11.1.0",
    "write-file-atomic": "^6.0.0",
    "ws": "^8.18.0",
    "xxhash-wasm": "^1.1.0",
    "yargs": "^18.0.0",
    "zlib": "^1.0.5",
    "zod": "^3.25.63"
  },
  "devDependencies": {
    "@babel/generator": "^7.27.5",
    "@babel/parser": "^7.27.5",
    "@babel/traverse": "^7.27.4",
    "@babel/types": "^7.27.6",
    "@eslint/js": "^9.28.0",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__traverse": "^7.20.7",
    "@types/node": "^24.0.1",
    "@types/uuid": "^10.0.0",
    "@types/write-file-atomic": "^4.0.3",
    "@types/ws": "^8.5.10",
    "@types/yargs": "^17.0.32",
    "@vitest/coverage-v8": "^3.2.3",
    "eslint": "^9.28.0",
    "globals": "^16.2.0",
    "memfs": "^4.17.2",
    "postcss": "^8.5.5",
    "prettier": "^3.3.1",
    "tailwindcss": "^3.4.4",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.34.0",
    "vite": "^6.3.5",
    "vitest": "^3.2.3",
    "webpack": "^5.99.9"
  }
}
