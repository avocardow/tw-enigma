{
  "name": "tw-enigma",
  "version": "0.1.0",
  "description": "A post-build optimizer for Tailwind CSS that bundles utility classes into single, obfuscated identifiers.",
  "author": "Rowan Cardow",
  "license": "MIT",
  "homepage": "https://github.com/avocardow/tw-enigma",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avocardow/tw-enigma.git"
  },
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "enigma": "dist/bin/enigma.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm build",
    "list": "node scripts/dev.js list",
    "generate": "node scripts/dev.js generate",
    "parse-prd": "node scripts/dev.js parse-prd",
    "license:add": "node scripts/add-license-headers.js",
    "license:check": "node scripts/add-license-headers.js --dry-run --verbose"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "cheerio": "^1.0.0-rc.12",
    "cli-table3": "^0.6.5",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "cosmiconfig": "8.1.3",
    "domhandler": "^5.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "fastmcp": "^1.20.5",
    "figlet": "^1.8.0",
    "fuse.js": "^7.0.0",
    "glob": "^10.2.7",
    "gradient-string": "^3.0.0",
    "helmet": "^8.1.0",
    "inquirer": "^12.5.0",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^10.2.0",
    "openai": "^4.89.0",
    "ora": "^8.2.0",
    "uuid": "^11.1.0",
    "write-file-atomic": "^6.0.0",
    "yargs": "^17.7.2",
    "zod": "^3.25.56"
  },
  "devDependencies": {
    "@babel/generator": "^7.27.5",
    "@babel/parser": "^7.27.5",
    "@babel/traverse": "^7.27.4",
    "@babel/types": "^7.27.6",
    "@eslint/js": "^9.28.0",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__parser": "^7.1.5",
    "@types/babel__traverse": "^7.20.7",
    "@types/node": "^20.14.2",
    "@types/uuid": "^10.0.0",
    "@types/yargs": "^17.0.32",
    "eslint": "^9.28.0",
    "globals": "^16.2.0",
    "postcss": "^8.4.38",
    "prettier": "^3.3.1",
    "tailwindcss": "^3.4.4",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.33.1",
    "vitest": "^1.6.0"
  }
}
